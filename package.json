{
  "name": "personal-application-studio",
  "displayName": "Personal Application Studio",
  "author": {
    "name": "Marcello Urbani",
    "email": "marcello.urbani@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcellourbani/personal-application-studio.git"
  },
  "theia": {
    "frontend": {
      "config": {
        "applicationName": "Theia JavaScript/TypeScript Example",
        "preferences": {
          "editor.autoSave": "on",
          "workspace.preserveWindow": true,
          "terminal.integrated.shell.linux": "fish"
        }
      }
    },
    "target": "browser"
  },
  "dependencies": {
    "@theia/callhierarchy": "latest",
    "@theia/debug": "latest",
    "@theia/editor-preview": "latest",
    "@theia/file-search": "latest",
    "@theia/getting-started": "latest",
    "@theia/git": "latest",
    "@theia/markers": "latest",
    "@theia/messages": "latest",
    "@theia/mini-browser": "latest",
    "@theia/navigator": "latest",
    "@theia/outline-view": "latest",
    "@theia/plugin": "latest",
    "@theia/plugin-ext": "latest",
    "@theia/plugin-ext-vscode": "latest",
    "@theia/preferences": "latest",
    "@theia/preview": "latest",
    "@theia/search-in-workspace": "latest",
    "@theia/terminal": "latest",
    "@theia/vsx-registry": "latest",
    "fs-extra": "^4.0.2",
    "yargs": "^11.1.0"
  },
  "devDependencies": {
    "@theia/cli": "latest"
  },
  "scripts": {
    "preinstall": "node-gyp install",
    "createimage": "docker build . -t murbani/personal-application-studio",
    "runcontainer": "docker run --name theia_pas -d -p 3000:3000 murbani/personal-application-studio",
    "pruneall": "docker kill theia_pas;docker rm theia_pas;docker image rm personal-application-studio",
    "logs": "docker logs -f theia_pas"
  },
  "resolutions": {
    "**/@theia/plugin-ext/decompress": "4.2.0",
    "**/@theia/preview/highlight.js": "10.4.1",
    "y18n": "5.0.5",
    "filenamify": "4.2.0"
  },
  "theiaPluginsDir": "plugins",
